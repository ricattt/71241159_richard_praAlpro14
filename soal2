# Soal 2
list1 = [1, 1, 2, 3, 3, 3, 4, 5, 5, 5, 5]
set1 = set(list1)
print('Sebelum:', list1)
print('Sesudah:', set1)
print()

set2 = {1, 2, 3, 4, 5, 6, 7}
list2 = list(set2)
print('Sebelum:', set2)
print('Sesudah:', list2)
print()

tuple3 = (7, 8, 8, 9, 10)
set3 = set(tuple3)
print('Sebelum:', tuple3)
print('Sesudah:', set3)
print()

set4 = {100, 200, 300}
tuple4 = tuple(set4)
print('Sebelum:', set4)
print('Sesudah:', tuple4)
